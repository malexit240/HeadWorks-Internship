  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Language.us.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Language.us.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Views\" />
  </ItemGroup>

</Project>